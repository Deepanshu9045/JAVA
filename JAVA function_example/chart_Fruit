import sys
import matplotlib
#matplotlib.use('Agg')

import matplotlib.pyplot as plt
import numpy as np

y = np.array([35,25,25,15,44])
mylabels = ["Apples","Bananas","Cherries","Dates","Berries"]
myexplode = [0.2, 0,0,0,0]
plt.pie(y, labels=mylabels, explode = mylabels, startangle =90,shadow=True )
plt.length(title ="Fruits")
plt.show()